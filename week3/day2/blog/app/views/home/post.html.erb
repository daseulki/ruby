<form action="/create">
  <div class="form-group">
    <label for="exampleFormControlInput0">글쓴이</label>
    <input type="text" class="form-control"  name="username" placeholder="이름이 뭐야아아아">
  </div>

  <div class="form-group">
    <label for="exampleFormControlInput1">제목</label>
    <input type="text" class="form-control"  name="title" placeholder="제목이 뭐야아아아">
  </div>

  <div class="form-group">
    <label for="exampleFormControlTextarea1">내용</label>
    <textarea name="content" class="form-control" placeholder="하고 싶은 말이 뭐야아아아"  rows="5"></textarea>
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
