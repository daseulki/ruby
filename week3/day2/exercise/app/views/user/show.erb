<table class = "table">
  <thead>
    <tr>
      <th scope="col"> 글번호</th>
      <th scope="col"> 글쓴이</th>
      <th scope="col"> 제목</th>
      <th scope="col"> 내용</th>
      <th scope="col"> 작성일자</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td> <%= @post.id%> </td>
      <td> <%= @post.username%> </td>
      <td> <%= @post.title%> </td>
      <td> <%= @post.content%> </td>
      <td> <%= @post.created_at %> </td>
    </tr>
  </tbody>
</table>
